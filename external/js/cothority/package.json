{
  "name": "@dedis/cothority",
  "version": "0.0.1",
  "description": "A typescript implementation of the cothority",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "ts-node node_modules/jasmine/bin/jasmine.js",
    "cover": "nyc node_modules/jasmine/bin/jasmine.js",
    "coveralls": "nyc --reporter=lcov node_modules/jasmine/bin/jasmine.js && cat ./coveralls/lcov.info | coveralls",
    "bundle": "node node_modules/.bin/webpack",
    "doc": "node_modules/.bin/typedoc --out doc/ src/",
    "prepublishOnly": "npm run build && npm run bundle && npm run doc",
    "protobuf": "node scripts/build_proto.js"
  },
  "author": "DEDIS",
  "license": "AGPL-3.0",
  "repository": "https://github.com/dedis/cothority.git",
  "dependencies": {
    "@stablelib/blake2xs": "^0.10.4",
    "@types/crypto-js": "^3.1.43",
    "@types/node": "^10.12.18",
    "@types/sprintf-js": "^1.1.1",
    "co": "^4.6.0",
    "file": "^0.2.2",
    "isomorphic-ws": "^4.0.1",
    "lodash": "^4.17.11",
    "os": "^0.1.1",
    "protobufjs": "^6.8.8",
    "pure-uuid": "^1.5.5",
    "shuffle-array": "^1.0.1",
    "sprintf-js": "^1.1.2",
    "toml": "^2.3.5",
    "util": "^0.11.1",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "@types/dockerode": "^2.5.11",
    "@types/jasmine": "^3.3.5",
    "dockerode": "^2.5.8",
    "jasmine": "^3.3.1",
    "ts-node": "^8.0.1",
    "typescript": "^3.1.6"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts"
    ],
    "exclude": [
      "spec/**/*",
      "**/*.d.ts",
      "**/*.config.js",
      "coverage/**/*",
      "dist/**/*",
      "doc/**/*",
      "spec/**/*"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
